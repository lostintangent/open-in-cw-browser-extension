{
  "manifest_version": 3,
  "name": "Open in Workspace",
  "version": "1.0",
  "description": "Select text on a webpage and open it in a GitHub workspace.",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
